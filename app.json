{
  "expo": {
    "name": "expense-tracker-react-native",
    "slug": "expense-tracker-react-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.alZuman.expenseTracker",
      "googleServicesFile": "./GoogleService-info.plist",
      "supportsTablet": true,
      "buildNumber": "0.0.4",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.894025729971-9jc4h5vd8u698uk0msm38i3hnvtpt4nb"
        }
      }
    },
    "android": {
      "package": "com.alZuman.expenseTracker",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
